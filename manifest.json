{
  "name": "Link Cleaner",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Cleaning URL encoding and removing tracking prefixes",
  "icons": {
        "16": "img/icon-16.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },

  "permissions": ["tabs"]
}
